{% block select2_widget %}
	{% spaceless %}
		{#{% for choices in choicess %}#}
			{#{{ choices }}#}
		{#{% endfor %}#}
		{% if expanded %}
			<ul {{ block('widget_container_attributes') }}>
				{% for child in form %}
					<li>
						{{ form_widget(child) }}
						{{ form_label(child) }}
					</li>
				{% endfor %}
			</ul>
		{% else %}
			{{ block('choice_widget') }}
		{% endif %}
	{% endspaceless %}
{% endblock %}