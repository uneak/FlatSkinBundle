{% extends '::base.html.twig' %}

{% block html_tag %}
<html lang="fr">{% endblock %}

	{% block title %}Administration - {{ parent() }}{% endblock %}
	{% block icon %}{{ asset('bundles/uneakflatskin/images/favicon.ico') }}{% endblock %}

	{% block meta_viewport %}width=device-width, initial-scale=1.0{% endblock %}
	{% block meta_keywords %}{% endblock %}
	{% block meta_description %}{% endblock %}
	{% block meta_author %}uneak{% endblock %}

	{% block content %}{% endblock %}

	{% block stylesheets %}

		<!-- Bootstrap core CSS -->

		<link href="{{ asset('bundles/uneakflatskin/css/bootstrap.min.css') }}" rel="stylesheet">
		<link href="{{ asset('bundles/uneakflatskin/css/bootstrap-reset.css') }}" rel="stylesheet">
		<!--external css-->
		<link href="{{ asset('bundles/uneakflatskin/assets/font-awesome/css/font-awesome.css') }}" rel="stylesheet"/>

		{% block stylesheets_page %}{% endblock %}

		<!--right slidebar-->
		<link href="{{ asset('bundles/uneakflatskin/css/slidebars.css') }}" rel="stylesheet">

		<!-- Custom styles for this template -->
		<link href="{{ asset('bundles/uneakflatskin/css/style.css') }}" rel="stylesheet">
		<link href="{{ asset('bundles/uneakflatskin/css/style-responsive.css') }}" rel="stylesheet"/>

	{% endblock %}

	{% block javascripts_top %}

		<!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
		<!--[if lt IE 9]>
		<script src="{{ asset('bundles/uneakflatskin/js/html5shiv.js') }}"></script>
		<script src="{{ asset('bundles/uneakflatskin/js/respond.min.js') }}"></script>
		<![endif]-->

	{% endblock %}

	{% block javascripts_bottom %}

		<!-- js placed at the end of the document so the pages load faster -->
		{#<script src="/bundles/uneakflatskin/js/jquery.js"></script>#}
		{#<script src="/bundles/uneakflatskin/js/bootstrap.min.js"></script>#}
		<script src="{{ asset('js/jquery.js') }}"></script>
		<script src="{{ asset('js/bootstrap.js') }}"></script>
		<script src="{{ asset('bundles/uneakflatskin/js/jquery.dcjqaccordion.2.7.js') }}" class="include" type="text/javascript"></script>
		<script src="{{ asset('bundles/uneakflatskin/js/jquery.scrollTo.min.js') }}"></script>
		<script src="{{ asset('bundles/uneakflatskin/js/slidebars.min.js') }}"></script>
		<script src="{{ asset('bundles/uneakflatskin/js/jquery.nicescroll.js') }}" type="text/javascript"></script>
		<script src="{{ asset('bundles/uneakflatskin/js/respond.min.js') }}"></script>

		<!--common script for all pages-->
		<script src="{{ asset('bundles/uneakflatskin/js/common-scripts.js') }}"></script>

		{% block javascripts_bottom_page %}{% endblock %}

	{% endblock %}

	{% block javascript_script %}{% endblock %}
