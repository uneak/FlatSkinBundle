<li class="{{ item.metas.class }}">
	<div class="media">
		<a href="#" class="pull-left media-thumb">
			<img alt="" src="{{ item.photo }}" class="media-object">
		</a>

		<div class="media-body">
			{% if item.badge %}
				<div class="media-status">
					<span class=" badge bg-important">{{ item.badge }}</span>
				</div>
			{% endif %}
			<strong>{{ item.title|raw }}</strong>
			<small>{{ item.subTitle }}</small>
		</div>
	</div>
</li>

